{
    "name": "@agentic-layer/n8n-nodes-a2a",
    "version": "0.1.0",
    "description": "n8n node for Agent-to-Agent (A2A) protocol communication",
    "license": "MIT",
    "homepage": "",
    "keywords": [
        "n8n-community-node-package",
        "a2a",
        "agent-to-agent",
        "ai-agents"
    ],
    "author": {
        "name": "Nicolai Ommer",
        "email": "nicolai.ommer@gmail.com"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/agentic-layer/n8n-nodes-a2a.git"
    },
   "scripts": {
		"build": "n8n-node build",
		"build:watch": "tsc --watch",
		"dev": "n8n-node dev",
		"lint": "n8n-node lint",
        "lint:fix": "n8n-node lint --fix",
		"release": "n8n-node release",
		"prepublishOnly": "n8n-node prerelease"
	},
    "files": [
        "dist"
    ],
    "n8n": {
        "n8nNodesApiVersion": 1,
        "strict": true,
        "credentials": [
            "dist/credentials/A2aApi.credentials.js"
        ],
        "nodes": [
            "dist/nodes/A2a/A2a.node.js"
        ]
    },
    "devDependencies": {
        "@n8n/node-cli": "*",
        "eslint": "9.32.0",
        "prettier": "3.6.2",
        "release-it": "^19.0.4",
        "typescript": "5.9.2"
    },
    "peerDependencies": {
        "n8n-workflow": "*"
    }
}
